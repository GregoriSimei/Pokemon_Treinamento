    agent any

    stages 
    {

            stage('Install') 
            {
                steps
                {
                    sh 'npm install'
                }
            }

            stage('Build')
            {
                steps 
                {
                    sh 'ng build --prod'
                }
            }

            stage('Deployment')
            {
                steps
                {
                    sh 'echo oi'
                }
            }
    
    }

}